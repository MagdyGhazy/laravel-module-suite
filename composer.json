{
    "name": "ghazym/laravel-module-suite",
    "description": "A comprehensive Laravel package for building modular applications with RBAC, media management, and API endpoints",
    "type": "library",
    "license": "MIT",
    "version": "2.0.0",
    "authors": [
        {
            "name": "Magdy Ghazy",
            "email": "magdyghazy04@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Ghazym\\LaravelModuleSuite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ghazym\\LaravelModuleSuite\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ghazym\\LaravelModuleSuite\\LaravelModuleSuiteServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^10.0|^11.0|^12.0",
        "phpunit/phpunit": "^10.0|^11.0|^12.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}