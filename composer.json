{
    "name": "ghazym/module-builder",
    "description": "A Laravel package for building custom modules with Role-Based Access Control (RBAC) and media management",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Magdy Ghazy",
            "email": "magdyghazy04@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Ghazym\\ModuleBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ghazym\\ModuleBuilder\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ghazym\\ModuleBuilder\\ModuleBuilderServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^10.0|^11.0|^12.0",
        "phpunit/phpunit": "^10.0|^11.0|^12.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.2.0"
}