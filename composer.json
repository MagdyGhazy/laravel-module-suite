{
    "name": "ghazym/module-builder",
    "description": "A Laravel package for custom module building",
    "type": "laravel-package",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Magdy Ghazy",
            "email": "magdyghazy04@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Ghazym\\ModuleBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ghazym\\ModuleBuilder\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ghazym\\ModuleBuilder\\ModuleBuilderServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}